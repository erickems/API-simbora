{
    "version": 28,
    "builds":[
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/index.js"
        },
        {
            "src": "/clientes",
            "methods": [
                "GET"
            ],
            "dest": "/index.js"
        }
    ]
}